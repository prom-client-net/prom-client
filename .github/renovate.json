{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    "group:monorepos",
    "workarounds:all"
  ],
  "labels": [
    "dependencies"
  ],
  "assignees": [
    "@phnx47"
  ],
  "ignoreDeps": [
    "System.Text.Encodings.Web",
    "xunit.runner.visualstudio"
  ],
  "packageRules": [
    {
      "automerge": true,
      "sourceUrlPrefixes": [
        "https://github.com/coverlet-coverage"
      ],
      "groupName": "coverlet packages"
    }
  ]
}
